<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>GrizzlyStore</display-name>
	<servlet>
		<servlet-name>AddProductServlet</servlet-name>
		<display-name>AddProductServlet</display-name>
		<description></description>
		<servlet-class>com.cognizant.GrizzlyStore.controller.AddProductServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListProductServlet</servlet-name>
		<display-name>ListProductServlet</display-name>
		<description></description>
		<servlet-class>com.cognizant.GrizzlyStore.controller.ListProductServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.cognizant.GrizzlyStore.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogutServlet</servlet-name>
		<display-name>LogutServlet</display-name>
		<description></description>
		<servlet-class>com.cognizant.GrizzlyStore.controller.LogutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewProductServlet</servlet-name>
		<display-name>ViewProductServlet</display-name>
		<description></description>
		<servlet-class>com.cognizant.GrizzlyStore.controller.ViewProductServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RedirectToAddProductServlet</servlet-name>
		<display-name>RedirectToAddProductServlet</display-name>
		<description></description>
		<servlet-class>com.cognizant.GrizzlyStore.controller.RedirectToAddProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddProductServlet</servlet-name>
		<url-pattern>/AddProductServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListProductServlet</servlet-name>
		<url-pattern>/ListProductServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogutServlet</servlet-name>
		<url-pattern>/LogutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewProductServlet</servlet-name>
		<url-pattern>/ViewProductServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RedirectToAddProductServlet</servlet-name>
		<url-pattern>/RedirectToAddProductServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
